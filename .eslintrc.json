{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import": 0,
        "max-len": 1,
        "no-alert": 1,
        "no-shadow": 1,
        "no-console": 1,
        "comma-dangle": 0,
        "no-continue":"off",
        "import/no-cycle": 0,
        "implicit-arrow-linebreak": 0,
        "dot-notation": "off",
        "no-return-assign": 0,
        "consistent-return": 1,
        "no-param-reassign": 0,
        "linebreak-style": "off",
        "object-curly-newline": "off",
        "no-use-before-define": 0,
        "no-underscore-dangle": 0,
        "prefer-regex-literals": 0,
        "import/prefer-default-export": 0,
        "import/no-dynamic-require": 1,
        "no-prototype-builtins": 1,
        "no-nested-ternary": 1,
        "prefer-promise-reject-errors": 0,
        "eqeqeq": "off",
        "prefer-const": "off",
        "no-unused-vars": [
            1,
            {
                "ignoreRestSiblings": false
            }
        ]
    }
}
